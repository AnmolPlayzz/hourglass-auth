{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "public": "public/**/*",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.html)",
      "dest": "/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}